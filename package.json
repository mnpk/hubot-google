{
  "name": "hubot-google",
  "version": "0.1.1",
  "description": "Search on google and get the first result",
  "main": "index.coffee",
  "scripts": {
    "test": "mocha",
    "test-html-cov": "mocha -r blanket -R html-cov",
    "coveralls": "mocha -r blanket -R mocha-lcov-reporter | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mnpk/hubot-google.git"
  },
  "keywords": [
    "hubot",
    "google",
    "search",
    "bot"
  ],
  "author": "mnpk <mnncat@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mnpk/hubot-google/issues"
  },
  "homepage": "https://github.com/mnpk/hubot-google",
  "dependencies": {
  },
  "devDependencies": {
    "tbot": "^0.1.0",
    "mocha": "^2.2.4",
    "coffee-script": "^1.9.2",
    "mocha-lcov-reporter": "0.0.2",
    "blanket": "alex-seville/blanket",
    "coveralls": "^2.11.2"
  },
  "config": {
    "blanket": {
      "loader": "./node-loaders/coffee-script",
      "data-cover-never": "node_modules",
      "data-cover-only": "src"
    }
  }
}
